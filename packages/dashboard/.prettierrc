{
  "useTabs": false,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "none",
  "printWidth": 100,
  "plugins": [
    "./node_modules/prettier-plugin-svelte/plugin.js",
    "./node_modules/prettier-plugin-tailwindcss/dist/index.mjs"
  ],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte"
      }
    }
  ]
}
