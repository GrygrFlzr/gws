{
  "name": "@gws/core",
  "version": "0.0.1",
  "type": "module",
  "description": "Shared core functionality for GWS",
  "exports": {
    ".": {
      "import": "./src/index.ts"
    },
    "./db": {
      "import": "./src/db/index.ts"
    },
    "./db/schema": {
      "import": "./src/db/schema.ts"
    },
    "./db/client": {
      "import": "./src/db/client.ts"
    },
    "./twitter": {
      "import": "./src/twitter/index.ts"
    },
    "./queue": {
      "import": "./src/queue/index.ts"
    }
  },
  "scripts": {
